{
  "name": "Open in specific window",
  "version": "1.2.1",
  "manifest_version": 2,
  "description": "Opens a link in a specified window",
  "permissions": [
    "tabs",
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Name this window",
    "default_popup": "browserAction.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}
